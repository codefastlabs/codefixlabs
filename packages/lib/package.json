{
  "name": "@codefixlabs/lib",
  "version": "0.1.16",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "tsup --watch",
    "lint": "eslint src/"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-codefixlabs/react"
    ]
  },
  "dependencies": {
    "libphonenumber-js": "^1.10.49",
    "react": "^18.2.0",
    "rehype-format": "^5.0.0",
    "rehype-parse": "^9.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-remark": "^10.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "@codefixlabs/tsconfig": "workspace:*",
    "@types/react": "^18.2.36",
    "eslint-config-codefixlabs": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
