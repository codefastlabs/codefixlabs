    "dev": "tsup --watch",
    "lint": "eslint src/"
  },
  "dependencies": {
    "libphonenumber-js": "^1.10.58",
    "react": "^18.2.0",
    "rehype-format": "^5.0.0",
    "rehype-parse": "^9.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-remark": "^10.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "@codefixlabs/tsconfig": "workspace:*",
    "@types/react": "^18.2.66",
    "eslint-config-codefixlabs": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
